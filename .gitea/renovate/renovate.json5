{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "docker:enableMajor",
    "replacements:k8s-registry-move",
    ":automergeBranch",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":skipStatusChecks",
    ":timezone(America/New_York)",
    "local>allowedVersions.json5",
    "local>autoMerge.json5",
    "local>clusters.json5",
    "local>commitMessage.json5",
    "local>customManagers.json5",
    "local>groups.json5",
    "local>labels.json5",
    "local>packageRules.json5",
    "local>semanticCommits.json5"
  ],
  "dependencyDashboardTitle": "Renovate Dashboard ðŸ¤–",
  "suppressNotifications": ["prEditedNotification", "prIgnoreNotification"],
  "onboarding": false,
  "requireConfig": "ignored",
  "ignorePaths": ["**/*.sops.*", "**/.archive/**", "**/resources/**"],
  "flux": {
    "fileMatch": [
      "(^|/)kubernetes/.+\\.ya?ml(?:\\.j2)?$"
    ]
  },
  "helm-values": {
    "fileMatch": [
      "(^|/)kubernetes/.+\\.ya?ml(?:\\.j2)?$"
    ]
  },
  "kubernetes": {
    "fileMatch": [
      "(^|/)kubernetes/.+\\.ya?ml(?:\\.j2)?$"
    ]
  }
}