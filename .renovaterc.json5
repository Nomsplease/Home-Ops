{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "docker:enableMajor",
    "helpers:pinGitHubActionDigests",
    // "github>Nomsplease/Home-Ops//.renovate/allowedVersions.json5",
    "github>Nomsplease/Home-Ops//.renovate/autoMerge.json5",
    "github>Nomsplease/Home-Ops//.renovate/customManagers.json5",
    // "github>Nomsplease/Home-Ops//.renovate/grafanaDashboards.json5",
    "github>Nomsplease/Home-Ops//.renovate/groups.json5",
    "github>Nomsplease/Home-Ops//.renovate/labels.json5",
    "github>Nomsplease/Home-Ops//.renovate/semanticCommits.json5",
    ":automergeBranch",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":timezone(America/New_York)"

  ],
  "dependencyDashboardTitle": "Renovate Dashboard ðŸ¤–",
  "suppressNotifications": [
    "prEditedNotification",
    "prIgnoreNotification"
  ],
  "ignorePaths": [
    "**/.archive/**",
    "**/*.sops.*",
    "**/resources/**"
  ],
  "flux": {
    "managerFilePatterns": ["/(^|/)kubernetes/.+\\.ya?ml$/"]
  },
  "helm-values": {
    "managerFilePatterns": ["/(^|/)kubernetes/.+\\.ya?ml$/"]
  },
  "kubernetes": {
    "managerFilePatterns": ["/(^|/)kubernetes/.+\\.ya?ml$/"]
  }
}